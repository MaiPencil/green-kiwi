#establish vectors from the transposed matrices that are aligned according to the number of rows
a=replicate(10,as.integer(abs(rnorm(10)*10)))
b=replicate(10,as.integer(abs(rnorm(100)*100)))
#defined transposed matrices that are multiplied with their corresponding vectors
A2=tA*a
B2=tB*b
#convert the vectors into matrices
a2=matrix(a,nrow=10)
b2=matrix(b,nrow=10)
#define the multiplication of matrices
A3=a2%*%A2
B3=b2%*%B2
#find the determinant and inverse of each matrix A3 and B3
det(A3)
[1] 1.035876e+37
solve(A3)
